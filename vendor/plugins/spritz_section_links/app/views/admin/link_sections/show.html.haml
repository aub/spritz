- content_for :head do
  = stylesheet_link_tag "links", :plugin => "spritz_section_links"

.box
  %h2.boxheader Links
  %ul.cleanlist
    - @section.links.each do |link|
      %li.boxcontent
        = link.url
        = link.title
        = link_to 'Edit', edit_admin_link_section_link_path(@section, link)
        = link_to 'Delete', admin_link_section_link_path(@section, link), :method => :delete
  
= link_to 'New Link', new_admin_link_section_link_path(@section)